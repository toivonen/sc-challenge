block b-playlist {

    js: true

    content: [
        {
            elem: 'play'
        },
        {
            elem: 'title',
            content: this.ctx.title
        },
        {
            elem: 'songs'
        }
    ]

    elem track, content: {
        elem: 'title',
        content: this.ctx.title
    },
    elem play, content: 'press to play'

}
