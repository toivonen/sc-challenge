block b-playlist {

    js: true

    content: [
        {
            elem: 'play',
            mods: { state: 'silent' }
        },
        {
            elem: 'title',
            content: this.ctx.title
        },
        {
            elem: 'songs'
        }
    ]

    elem track, content: [
        {
            elem: 'trash'
        },
        {
            elem: 'title',
            content: this.ctx.title
        }
    ]

    elem play, tag: 'span',
    elem trash, tag: 'span'

}
