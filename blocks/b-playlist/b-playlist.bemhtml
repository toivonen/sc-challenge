block b-playlist {

    js: true

    content: [
        {
            elem: 'play',
            mods: { state: 'silent' }
        },
        {
            elem: 'title',
            content: this.ctx.title
        },
        {
            elem: 'songs'
        }
    ]

    elem track, content: {
        elem: 'title',
        content: this.ctx.title
    },
    elem play, tag: 'span'

}
